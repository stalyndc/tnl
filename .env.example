# Application configuration overrides
# Duplicate this file to `.env` and adjust values as needed.

# Cache TTL in seconds (default 1800)
APP_CACHE_TTL=1800

# Cache cleanup max age in seconds (default 604800)
APP_CACHE_CLEANUP_MAX_AGE=604800

# HTTP timeout for feed requests in seconds (default 10)
APP_HTTP_TIMEOUT=10

# Feed admin authentication
FEED_ADMIN_USER=
FEED_ADMIN_PASS=
# Optional token used by CLI (`--token=` argument)
FEED_ADMIN_TOKEN=
